<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--虚化背景-->
    <declare-styleable name="gallery_BlurredView">
        <attr name="gallery_src" format="reference" />
        <attr name="gallery_move" format="boolean" />
        <attr name="gallery_disableBlurred" format="boolean" />
    </declare-styleable>
    <!--圆形头像-->
    <declare-styleable name="gallery_roundedimageview">
        <!-- 圆形图片 -->
        <attr name="gallery_border_width" format="dimension" />
        <attr name="gallery_border_color" format="color" />
        <attr name="gallery_border_overlay" format="boolean" />
    </declare-styleable>





    <attr name="SwipeBackLayoutStyle" format="reference" />
    <!-- SwipeBackLayout -->
    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension" />
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference" />
        <attr name="shadow_right" format="reference" />
        <attr name="shadow_bottom" format="reference" />
    </declare-styleable>


    <style name="SwipeBackLayout">
        <item name="edge_size">0dp</item>
    </style>

    <!--横向进度条-->
    <declare-styleable name="HorizontalProgressBarWithNumber">
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />
        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_offset" format="dimension" />
        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <!--圆形进度条-->
    <declare-styleable name="RoundProgressBarWidthNumber">
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <!--photoview的 -->
    <declare-styleable name="ucrop_UCropView">
        <!--Crop image view-->
        <attr name="ucrop_aspect_ratio_x" format="float" />
        <attr name="ucrop_aspect_ratio_y" format="float" />
        <!--Overlay view-->
        <attr name="ucrop_show_oval_crop_frame" format="boolean" />
        <attr name="ucrop_oval_dimmed_layer" format="boolean" />
        <attr name="ucrop_dimmed_color" format="color" />

        <attr name="ucrop_grid_stroke_size" format="dimension" />
        <attr name="ucrop_grid_color" format="color" />
        <attr name="ucrop_grid_row_count" format="integer" />
        <attr name="ucrop_grid_column_count" format="integer" />
        <attr name="ucrop_show_grid" format="boolean" />

        <attr name="ucrop_frame_stroke_size" format="dimension" />
        <attr name="ucrop_frame_color" format="color" />
        <attr name="ucrop_show_frame" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ucrop_AspectRatioTextView">

        <attr name="ucrop_artv_ratio_title" format="string"/>

        <attr name="ucrop_artv_ratio_x" format="float"/>
        <attr name="ucrop_artv_ratio_y" format="float"/>

    </declare-styleable>


    <!--标题栏颜色-->
    <attr name="gallery_toolbar_bg" format="color"/>
    <!--标题栏关闭按钮图片-->
    <attr name="gallery_toolbar_close_image" format="reference"/>
    <!--标题栏关闭按钮图片颜色-->
    <attr name="gallery_toolbar_close_color" format="color"/>
    <!--标题栏控件颜色-->
    <attr name="gallery_toolbar_widget_color" format="color"/>
    <!--标题栏字体颜色-->
    <attr name="gallery_toolbar_text_color" format="color"/>
    <!--标题栏字体大小-->
    <attr name="gallery_toolbar_text_size" format="dimension"/>
    <!--标题栏分割线高度-->
    <attr name="gallery_toolbar_divider_height" format="dimension"/>
    <!--标题栏分割线背景-->
    <attr name="gallery_toolbar_divider_bg" format="reference|color"/>
    <!--标题栏bottomMargin-->
    <attr name="gallery_toolbar_bottom_margin" format="reference|dimension"/>
    <!--标题栏对齐方式-->
    <attr name="gallery_toolbar_text_gravity">
        <!-- Push object to the top of its container, not changing its size. -->
        <flag name="top" value="0x30" />
        <!-- Push object to the bottom of its container, not changing its size. -->
        <flag name="bottom" value="0x50" />
        <!-- Push object to the left of its container, not changing its size. -->
        <flag name="left" value="0x03" />
        <!-- Push object to the right of its container, not changing its size. -->
        <flag name="right" value="0x05" />
        <!-- Place object in the vertical center of its container, not changing its size. -->
        <flag name="center_vertical" value="0x10" />
        <!-- Grow the vertical size of the object if needed so it completely fills its container. -->
        <flag name="fill_vertical" value="0x70" />
        <!-- Place object in the horizontal center of its container, not changing its size. -->
        <flag name="center_horizontal" value="0x01" />
        <!-- Grow the horizontal size of the object if needed so it completely fills its container. -->
        <flag name="fill_horizontal" value="0x07" />
        <!-- Place the object in the center of its container in both the vertical and horizontal axis, not changing its size. -->
        <flag name="center" value="0x11" />
        <!-- Grow the horizontal and vertical size of the object if needed so it completely fills its container. -->
        <flag name="fill" value="0x77" />
        <!-- Additional option that can be set to have the top and/or bottom edges of
             the child clipped to its container's bounds.
             The clip will be based on the vertical gravity: a top gravity will clip the bottom
             edge, a bottom gravity will clip the top edge, and neither will clip both edges. -->
        <flag name="clip_vertical" value="0x80" />
        <!-- Additional option that can be set to have the left and/or right edges of
             the child clipped to its container's bounds.
             The clip will be based on the horizontal gravity: a left gravity will clip the right
             edge, a right gravity will clip the left edge, and neither will clip both edges. -->
        <flag name="clip_horizontal" value="0x08" />
        <!-- Push object to the beginning of its container, not changing its size. -->
        <flag name="start" value="0x00800003" />
        <!-- Push object to the end of its container, not changing its size. -->
        <flag name="end" value="0x00800005" />
    </attr>
    <!--标题栏高度-->
    <attr name="gallery_toolbar_height" format="dimension"/>
    <!--标题栏完成按钮背景-->
    <attr name="gallery_toolbar_over_button_bg" format="dimension"/>
    <!--标题栏完成按钮字体大小-->
    <attr name="gallery_toolbar_over_button_text_size" format="dimension"/>
    <!--标题栏完成按钮字体颜色-->
    <attr name="gallery_toolbar_over_button_text_color" format="color"/>
    <!--标题栏完成按钮使用默认背景为其设置点击颜色-->
    <attr name="gallery_toolbar_over_button_pressed_color" format="color"/>
    <!--标题栏完成按钮使用默认背景为其设置正常颜色-->
    <attr name="gallery_toolbar_over_button_normal_color" format="color"/>
    <!--系统栏颜色-->
    <attr name="gallery_color_statusbar" format="color"/>
    <!--控件激活,已选择状态颜色-->
    <attr name="gallery_color_active_widget" format="color"/>
    <!--设置checkbox,radiobutton buttonTint颜色-->
    <attr name="gallery_checkbox_button_tint_color" format="color"/>
    <!--checkbox文本颜色-->
    <attr name="gallery_checkbox_text_color" format="color"/>
    <!--页面背景-->
    <attr name="gallery_page_bg" format="color"/>
    <!--默认图-->
    <attr name="gallery_default_image" format="dimension"/>
    <!--图片背景-->
    <attr name="gallery_imageview_bg" format="dimension|color"/>
    <!--拍照图片-->
    <attr name="gallery_camera_image" format="dimension"/>
    <!--拍照图片背景-->
    <attr name="gallery_camera_bg" format="dimension|color"/>
    <!--拍照文字颜色-->
    <attr name="gallery_take_image_text_color" format="color"/>
    <!--请求读取相册授权提示语-->
    <attr name="gallery_request_storage_access_permission_tips" format="dimension|string"/>
    <!--没有任何图片提示语-->
    <attr name="gallery_media_empty_tips" format="dimension|string"/>

    <!---uCrop-->
    <!--裁剪页背景颜色-->
    <attr name="gallery_ucrop_color_crop_background" format="color"/>
    <!--图片外框背景颜色-->
    <attr name="gallery_ucrop_image_outer_frame_bg" format="color"/>
    <!--裁剪边框颜色-->
    <attr name="gallery_ucrop_crop_frame_color" format="color"/>
    <!--裁剪边框宽度-->
    <attr name="gallery_ucrop_crop_frame_stroke_width" format="dimension"/>
    <!--裁剪框宫格线颜色-->
    <attr name="gallery_ucrop_image_grid_color" format="color"/>
    <!--裁剪框宫格线框-->
    <attr name="gallery_ucrop_image_grid_stroke_size" format="dimension"/>
    <attr name="gallery_ucrop_vector_ic_crop" format="dimension"/>
    <!--裁剪页标题文本-->
    <attr name="gallery_ucrop_title_text" format="string"/>
    <!--是否显示裁剪页面frame-->
    <attr name="gallery_ucrop_show_crop_frame" format="boolean"/>
    <!--是否显示裁剪页面九宫格线-->
    <attr name="gallery_ucrop_show_crop_grid" format="boolean"/>
    <!--裁剪页面九宫格线行数-->
    <attr name="gallery_ucrop_show_crop_grid_row_count" format="integer"/>
    <!--裁剪页面九宫格线列数-->
    <attr name="gallery_ucrop_show_crop_grid_column_count" format="integer"/>
</resources>